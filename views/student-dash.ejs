<!doctype html>

<html lang="en">

<head>
    <meta charset="utf-8">
    <title>student dash ejs</title>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet"
        integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3" crossorigin="anonymous">

    <link rel="stylesheet" type="text/css" href="../stylesheet.css" />
</head>

<header>
    <%- include('z-s-nav') %>

</header>

<body class="bg-dark">
    <div class="row">
        <div class="col-lg-12 ">
            <div class="row">
                <div class="col-lg-12 ">
                    <h1 class="text-warning text-center">Student Dash</h1>
                    <%#=allStudentInfo%>
                    
                </div>
            </div>
            <div class="container p-5 m-5 h-100">
                <div class="row">
                    <div class="col-lg-12 ">
                        <div class="card card-registration">


                            <h1 class="text-center">Current Courses</h1>
                            <div class="row approvedCourseList">
                                <div class="col-auto">
                                    <table class="table">
                                        <thead class="thead-dark">
                                            <tr>
                                                <th scope="col">Course subject</th>
                                                <th scope="col">Course number</th>
                                                <th scope="col">Course Name</th>
                                                <th scope="col">Course Term</th>
                                                <th scope="col">Course Instructor</th>
                                                <th scope="col">More Info Link </th>
                                                <th scope="col">Course Work Link</th>
                                            </tr>
                                        </thead>
                                        <tbody>

                                        </tbody>
                                        <tr>

                                        </tr>

                                        <!--Populate table based on javascript student course list -->
                                    </table>



                                    <h1 class="pt-5 text-center">Waiting List Courses</h1>
                                    <div class="waitingListCourses">
                                        <table class="table">

                                            <tr>
                                                <th scope="col">Course subject</th>
                                                <th scope="col">Course number</th>
                                                <th scope="col">Course Name</th>
                                                <th scope="col">Course Term</th>
                                                <th scope="col">Course Instructor</th>
                                                <th scope="col">More Info Link</th>
                                            </tr>
                                            <tbody>

                                            </tbody>
                                            <tr>

                                            </tr>
                                            <!--Populate table based on javascript student course list -->
                                        </table>
                                    </div>
                                </div>
                            </div>

                        </div>
                    </div>
                </div>
            </div>

        </div>
    </div>





    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.bundle.min.js"
        integrity="sha384-ka7Sk0Gln4gmtz2MlQnikT1wXgYsOg+OMhuP+IlRH9sENBO0LRn5q+8nbTov4+1p"
        crossorigin="anonymous"></script>
</body>




</html>